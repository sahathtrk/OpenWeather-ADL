{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Geocoding Response",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "local_names": {
        "type": "object",
        "properties": {
          "en": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        },
        "required": ["en", "id"]
      },
      "lat": {
        "type": "number"
      },
      "lon": {
        "type": "number"
      },
      "country": {
        "type": "string"
      }
    },
    "required": ["name", "local_names", "lat", "lon", "country"]
  }
}
